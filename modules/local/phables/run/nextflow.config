process {
    withName: PHABLES_RUN {
        ext.args = [
            // '--no-use-conda',
            params.phables_args ? "${params.phables_args}" : ''
        ].join(' ').trim()
        publishDir = [
            path: { "${params.outdir}/VirusExtension/phables/run" },
            mode: params.publish_dir_mode,
            pattern: "*fasta.gz"
        ]
    }
}
