process {
    withName: 'ASPERACLI' {
        ext.args = '-QT -l 300m -P33001'
        ext.prefix = { "${meta.id}_run${meta.run}"}
        publishDir = [
            [
                path: { "${params.outdir}/SRADownload/asperacli" },
                mode: params.publish_dir_mode,
                pattern: "*.fastq.gz"
            ],
            [
                path: { "${params.outdir}/SRADownload/asperacli" },
                mode: params.publish_dir_mode,
                pattern: "*.md5"
            ]
        ]
    }
}
