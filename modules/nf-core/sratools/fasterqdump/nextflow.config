process {
    withName: SRATOOLS_FASTERQDUMP {
        ext.args = '--split-files --include-technical'
        ext.prefix = { "${meta.id}_run${meta.run}"}
        publishDir = [
            path: { "${params.outdir}/SRADownload/sratools/fasterqdump" },
            mode: params.publish_dir_mode,
            pattern: "*.fastq.gz"
        ]
    }
}