nextflow_process {

    name "Test Process IPHOP_DOWNLOAD"
    script "../main.nf"
    process "IPHOP_DOWNLOAD"

    test("iphop_test_db") {
        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }
    }

    test("iphop_test_db - stub") {

        options "-stub"

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }
    }
}
